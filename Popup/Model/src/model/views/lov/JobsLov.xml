<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="JobsLov"
  Version="12.2.1.9.14"
  InheritPersonalization="merge"
  SelectList="Jobs.JOB_ID, 
       Jobs.JOB_TITLE, 
       Jobs.MAX_SALARY, 
       Jobs.MIN_SALARY"
  FromList="JOBS Jobs"
  Where="Jobs.MIN_SALARY &lt;= nvl(:BCurrentSalary, Jobs.MIN_SALARY)"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full">
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.views.lov.JobsLov_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BCurrentSalary"
    Kind="where"
    Type="java.math.BigDecimal">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <EntityUsage
    Name="Jobs"
    Entity="model.entities.Jobs"
    ReadOnly="true"
    DeleteParticipant="false"/>
  <ViewAttribute
    Name="JobId"
    IsUpdateable="false"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="JobId"
    EntityUsage="Jobs"
    AliasName="JOB_ID"/>
  <ViewAttribute
    Name="JobTitle"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="JobTitle"
    EntityUsage="Jobs"
    AliasName="JOB_TITLE"/>
  <ViewAttribute
    Name="MaxSalary"
    PrecisionRule="true"
    EntityAttrName="MaxSalary"
    EntityUsage="Jobs"
    AliasName="MAX_SALARY"/>
  <ViewAttribute
    Name="MinSalary"
    PrecisionRule="true"
    EntityAttrName="MinSalary"
    EntityUsage="Jobs"
    AliasName="MIN_SALARY"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
